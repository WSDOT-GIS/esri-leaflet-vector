{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": ["eslint-config-semistandard", "plugin:jsdoc/recommended"],
  "plugins": [
    "chai-friendly",
    "jsdoc"
  ],
  "rules": {
    "no-var": "off",
    "no-unused-expressions": 0,
    "chai-friendly/no-unused-expressions": 2
  },
  "globals": {
    "L": false
  },
  "ignorePatterns": [
    "eslintrc.js",
    "dist"
  ]
}